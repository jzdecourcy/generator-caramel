{
	"name": "ASP.NET",
	"private": true,
	"dependencies": {
		"bootstrap": "^3.3.6",
		"jquery-validation": "jquery.validation#^1.14.0",
		"jquery-validation-unobtrusive": "^3.2.5",
		"bootstrap-datepicker": "^1.6.0",
		"jquery.maskedinput": "^1.4.1",
		"bootbox.js": "bootbox#^4.4.0"
	},
	"overrides": {
		"bootstrap": {
			"main": [
